{{extend 'layout.html'}}
{{if  results:}}
 <table>
  <tr>
   {{for c in columns:}}
    <th class="result">{{=c}}</td>
   {{pass}}
  </tr>
   {{for r in results:}}
   <tr>
    <td>{{=A("View", _href=URL(r=request, f="show", a=r.id))}}</td>
    {{for c in columns:}}
     <td class="result">{{=r[names[c]]}}</td>
    {{pass}}

   </tr>
   {{pass}}
</table>
{{pass}}
{{=form}}
